{
    "name": "Quotemark",
    "version": "1.0",
    "description": "Save quotes into an extension!",
    "permissions": ["tabs", "contextMenus","activeTab","declarativeContent","storage"],
    "background": {
        "scripts": ["background.js", "contextmenu.js"]
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon":"icon512x512.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "all_frames": true,
        "js": ["contentscript.js"]
        }
    ],
    "icons" : {
        "16": "icon16x16.png"
    },
    "manifest_version": 2
  }

         