{
    "name": "Quotemark",
    "version": "1.0",
    "description": "Save quotes into an extension!",
    "permissions": ["storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
  }